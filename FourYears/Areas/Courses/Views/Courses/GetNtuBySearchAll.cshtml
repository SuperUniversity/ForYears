@model IEnumerable<MvcClient.Areas.Courses.Models.NtuCourseModel>


<table class="table">
    <tr>
        <th hidden="hidden">
            @Html.DisplayNameFor(model => model._id)
        </th>
        <th style="width:15%">
            @Html.DisplayNameFor(model => model.課程名稱)
        </th>
        <th style="width:7%">
            @Html.DisplayNameFor(model => model.授課教師)
