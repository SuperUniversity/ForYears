            @Html.DisplayFor(modelItem => item.學分)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.必選修)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.總人數)
        </td>
