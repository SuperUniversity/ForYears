        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.全半年)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.學分)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.必選修)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.總人數)
        </th>
        <th  style="width:20%">
            @Html.DisplayNameFor(model => model.選課限制條件)
