@model IEnumerable<FourYears.Areas.Courses.ViewModel.UserRankingViewModel>

@{
    ViewBag.Title = "使用者評價一覽";
}

<h2>使用者評價一覽</h2>


<table class="table">
    <tr>
        <th hidden="hidden">
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NickName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th hidden="hidden">
            @Html.DisplayNameFor(model => model.CourseId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CourseName)
        </th>
        <th hidden="hidden">
            @Html.DisplayNameFor(model => model.RankingId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastModified)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.deepness)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.relaxing)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sweetness)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td hidden="hidden">
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NickName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td hidden="hidden">
            @Html.DisplayFor(modelItem => item.CourseId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseName)
        </td>
        <td hidden="hidden">
            @Html.DisplayFor(modelItem => item.RankingId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastModified)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.deepness)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.relaxing)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sweetness)
        </td>
        <td>
            @Html.ActionLink("刪除", "DeleteRanking", new { userRankingId = item.User_RankingId })
        </td>
    </tr>
}

</table>
